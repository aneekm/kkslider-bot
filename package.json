{
  "name": "kkslider",
  "version": "1.0.0",
  "description": "mediamigos discord music bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "watch": "npm run build -- -w",
    "start": "npm run build && node dist/index.js",
    "deploy": "npm run build && node dist/deploy-commands.js",
    "deploy global": "npm run build && node dist/deploy-commands.js global",
    "deploy clean": "npm run build && node dist/deploy-commands.js clean",
    "test": "echo \"K.K. Slider doesn't need a warmup!\" && exit 1"
  },
  "author": "",
  "license": "GPLV3",
  "dependencies": {
    "@discordjs/builders": "^0.9.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.7.5",
    "@types/node": "^16.11.6",
    "discord-api-types": "^0.25.2",
    "discord.js": "^13.3.1",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.4.4",
    "ytdl-core-discord": "^1.3.1",
    "ytpl": "^2.2.3",
    "ytsr": "^3.5.3"
  }
}
